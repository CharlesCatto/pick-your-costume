{
	"name": "pick-your-costume-server",
	"version": "1.0.0",
	"description": "Backend server for Pick Your Costume",
	"main": "dist/main.js",
	"type": "commonjs",
	"scripts": {
		"dev": "nodemon src/main.ts",
		"build": "tsc",
		"start": "node dist/main.js",
		"migrate": "ts-node bin/migrate.ts",
		"seed": "ts-node bin/seed.ts",
		"reset-db": "npm run migrate && npm run seed",
		"check-db": "ts-node bin/check-db.ts",
		"render-build": "npm install && npm run build"
	},
	"keywords": [
		"express",
		"typescript",
		"mysql",
		"costume"
	],
	"author": "Charles Cattoir",
	"license": "ISC",
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^16.6.1",
		"express": "^4.19.2",
		"mysql2": "^3.14.5"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^20.12.12",
		"nodemon": "^3.1.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.5"
	}
}
